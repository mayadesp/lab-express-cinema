<h2>Ironhack cinema</h2>
<p>Click on the movie to check the showtimes!</p>


<ul class="movies-container">
    {{#each movies}}
    <li>
        <img class="images" src="{{this.image}}" alt="image">
        <h2>{{this.title}}</h2>
        <a href="movies/{{this.id}}">See more</a>

    </li>

    {{/each}}
</ul>